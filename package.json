{
    "name": "email-viewer",
    "displayName": "Email Viewer",
    "description": "Mounts email message files (EML & MSG) as workspace directories in VS Code and enables previewing them",
    "version": "5.0.0",
    "publisher": "TomasHubelbauer",
    "repository": {
        "type": "git",
        "url": "https://github.com/TomasHubelbauer/vscode-email-viewer.git"
    },
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "workspaceContains:**/*.eml",
        "workspaceContains:**/*.msg",
        "onFileSystem:eml",
        "onFileSystem:msg"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.45.0",
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "eslint": "^6.8.0",
        "@typescript-eslint/parser": "^2.30.0",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "glob": "^7.1.6",
        "mocha": "^7.1.2",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "@types/mailparser": "^2.0.2",
        "@types/pretty-bytes": "^5.1.0",
        "mailparser": "^2.7.7",
        "msgreader": "^1.0.1",
        "pretty-bytes": "^5.1.0"
    }
}
